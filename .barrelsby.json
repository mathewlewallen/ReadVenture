{
  "directory": ["./src"],
  "delete": true,
  "exclude": [
    "\\.test\\.",
    "\\.spec\\.",
    "__tests__",
    "__mocks__",
    "\\.env",
    "\\.config\\.",
    "\\.d\\.ts$"
  ],
  "include": [".*\\.(ts|tsx)$"],
  "location": "top",
  "structure": "flat",
  "singleQuotes": true,
  "noHeader": false,
  "header": "/**\n * @generated - Do not modify directly\n */",
  "exportFilename": false,
  "nameFormat": "${name.pascalCase"
}
