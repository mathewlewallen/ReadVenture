{
  "name": "readventure",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "NODE_OPTIONS=--max-old-space-size=8192 eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "NODE_OPTIONS=--max-old-space-size=8192 eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "react-native start",
    "test": "jest --config jest.config.ts --detectOpenHandles --forceExit",
    "prepare": "husky install",
    "generate-barrels": "barrelsby --config .barrelsby.json",
    "prestart": "npm run generate-barrels",
    "prebuild": "npm run generate-barrels"
  },
  "dependencies": {
    "@react-native-firebase/app": "18.7.3",
    "@react-native-firebase/auth": "18.7.3",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@reduxjs/toolkit": "^1.9.7",
    "react": "^18.2.0",
    "react-native": "^0.72.4",
    "react-native-config": "1.5.1",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-screens": "^3.29.0",
    "react-native-vector-icons": "^10.2.0",
    "react-redux": "^8.1.3"
  },
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@babel/plugin-transform-class-properties": "7.25.9",
    "@babel/plugin-transform-nullish-coalescing-operator": "7.25.9",
    "@babel/plugin-transform-optional-chaining": "7.25.9",
    "@babel/preset-env": "7.26.0",
    "@babel/runtime": "7.26.0",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native/babel-preset": "0.78.0-nightly-20241205-f402ed17f",
    "@react-native/eslint-config": "0.78.0-nightly-20241205-f402ed17f",
    "@react-native/metro-config": "0.78.0-nightly-20241205-f402ed17f",
    "@react-native/typescript-config": "0.78.0-nightly-20241205-f402ed17f",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.9.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "babel-jest": "29.7.0",
    "barrelsby": "^2.8.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-native": "^4.1.0",
    "husky": "^8.0.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.4.2",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.2",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
