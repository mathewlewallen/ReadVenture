{
  "name": "readventure",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "start": "react-native start",
    "test": "jest --config jest.config.ts --detectOpenHandles",
    "prepare": "husky install",
    "generate-barrels": "barrelsby --config .barrelsby.json",
    "prestart": "npm run generate-barrels",
    "prebuild": "npm run generate-barrels",
    "build:backend": "tsc -p backend/tsconfig.json",
    "start:backend": "node backend/dist/app.js",
    "build:backend-firebase": "tsc -p backend-firebase/functions/tsconfig.json",
    "start:backend-firebase": "node backend-firebase/functions/lib/index.js",
    "typecheck": "tsc --noEmit",
    "validate": "npm run format:check && npm run lint"
  },
  "dependencies": {
    "@react-native-firebase/app": "18.7.3",
    "@react-native-firebase/auth": "18.7.3",
    "@react-navigation/native": "6.1.9",
    "@react-navigation/native-stack": "6.9.17",
    "@reduxjs/toolkit": "1.9.7",
    "react": "18.2.0",
    "react-native": "0.72.7",
    "bcryptjs": "2.4.3",
    "body-parser": "1.20.2",
    "express": "4.18.2",
    "mongoose": "7.6.3"
  },
  "devDependencies": {
    "@babel/core": "7.23.3",
    "@babel/preset-env": "7.23.3",
    "@babel/runtime": "7.23.3",
    "@types/react": "18.2.37",
    "@types/react-native": "0.72.6",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "6.11.0",
    "jest": "29.7.0",
    "@types/jest": "29.5.8",
    "typescript": "5.2.2",
    "@types/node": "20.9.1",
    "eslint": "8.53.0",
    "prettier": "3.1.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": ["eslint --fix", "prettier --write"],
    "*.{json,md}": ["prettier --write"]
  }
}
