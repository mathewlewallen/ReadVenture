{
  "name": "readventure",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "NODE_OPTIONS=--max-old-space-size=8192 eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "NODE_OPTIONS=--max-old-space-size=8192 eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "react-native start",
    "test": "jest --config jest.config.ts --detectOpenHandles --forceExit",
    "prepare": "husky install",
    "generate-barrels": "barrelsby --config .barrelsby.json",
    "prestart": "npm run generate-barrels",
    "prebuild": "npm run generate-barrels",
    "build:backend": "tsc -p backend/tsconfig.json",
    "start:backend": "node backend/dist/app.js",
    "build:backend-firebase": "tsc -p backend-firebase/functions/tsconfig.json",
    "start:backend-firebase": "node backend-firebase/functions/lib/index.js"
  },
  "dependencies": {
    "@react-native-firebase/app": "21.6.1",
    "@react-native-firebase/auth": "21.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@reduxjs/toolkit": "^2.5.0",
    "react": "18.3.0",
    "react-native": "0.76.5",
    "react-native-config": "1.5.3",
    "react-native-gesture-handler": "^2.21.2",
    "react-native-safe-area-context": "^5.0.0",
    "react-native-screens": "^4.3.0",
    "react-native-vector-icons": "^10.2.0",
    "react-redux": "^9.1.2"
  },
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@babel/plugin-transform-class-properties": "7.25.9",
    "@babel/plugin-transform-nullish-coalescing-operator": "7.25.9",
    "@babel/plugin-transform-optional-chaining": "7.25.9",
    "@babel/preset-env": "7.26.0",
    "@babel/runtime": "7.26.0",
    "@react-native-async-storage/async-storage": "^2.1.0",
    "@react-native/babel-preset": "0.76.5",
    "@react-native/eslint-config": "0.76.5",
    "@react-native/metro-config": "0.76.5",
    "@react-native/typescript-config": "0.76.5",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.9.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.14",
    "@types/react-test-renderer": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "babel-jest": "29.7.0",
    "barrelsby": "^2.8.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-native": "^4.1.0",
    "husky": "^9.1.7",
    "jest": "29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.11",
    "prettier": "^3.4.2",
    "react-test-renderer": "^18.3.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "5.7.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": ["eslint --fix", "prettier --write"],
    "*.{json,md}": ["prettier --write"]
  }
}
