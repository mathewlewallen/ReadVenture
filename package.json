{
  "name": "readventure",
  "displayName": "ReadVenture",
  "version": "0.0.1",
  "description": "A React Native mobile application for interactive reading adventures",
  "private": true,
  "license": "none",
  "publisher": "mathewlewallen",
  "icon": "src/assets/icons/example.png",
  "galleryBanner": {
    "color": "#011627",
    "theme": "dark"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "prestart": "npm run generate-barrels",
    "prebuild": "npm run generate-barrels",
    "build:backend": "tsc -p backend/tsconfig.json",
    "build:backend-firebase": "tsc -p backend-firebase/functions/tsconfig.json",
    "start:backend": "node backend/dist/app.js",
    "start:backend-firebase": "node backend-firebase/functions/lib/index.js",
    "test": "jest --config jest.config.ts --detectOpenHandles",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "validate": "npm run format:check && npm run lint",
    "prepare": "husky install",
    "generate-barrels": "barrelsby --config .barrelsby.json"
  },
  "dependencies": {
    "@babel/plugin-transform-class-properties": "^7.25.9",
    "@babel/plugin-transform-nullish-coalescing-operator": "^7.25.9",
    "@babel/plugin-transform-optional-chaining": "^7.25.9",
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-native-firebase/analytics": "^21.6.1",
    "@react-native-firebase/app": "21.6.1",
    "@react-native-firebase/auth": "21.6.1",
    "@react-native-voice/voice": "^3.2.4",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "7.2.0",
    "@reduxjs/toolkit": "2.5.0",
    "@sentry/react-native": "^6.4.0",
    "@types/react-redux": "^7.1.34",
    "axios": "^1.7.9",
    "barrelsby": "^2.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-native": "0.76.5",
    "react-native-config": "^1.5.3",
    "react-native-paper": "^5.12.5",
    "react-native-progress": "^5.0.1",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.26.0",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.76.5",
    "@react-native/eslint-config": "0.76.5",
    "@react-native/metro-config": "0.76.5",
    "@react-native/typescript-config": "0.76.5",
    "@testing-library/react-native": "^12.9.0",
    "@types/axios-mock-adapter": "^1.9.0",
    "@types/jest": "29.5.14",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.1",
    "@types/react-native": "^0.73.0",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^18.3.1",
    "@types/redux-mock-store": "^1.5.0",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "axios-mock-adapter": "^2.1.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.57.1",
    "husky": "^9.1.7",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "^18.3.1",
    "redux-mock-store": "^1.5.5",
    "typescript": "5.0.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": ["eslint --fix", "prettier --write"],
    "*.{json,md}": ["prettier --write"]
  }
}
